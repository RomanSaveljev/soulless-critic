{
  "name": "soulless-critic",
  "version": "0.0.1",
  "description": "Runs code style checks and comments on Gerrit reviews",
  "main": "index.js",
  "repository": "RomanSaveljev/soulless-critic",
  "scripts": {
    "test": "make test-all",
    "travis-test": "make travis-test",
    "cov": "make cov-all"
  },
  "config": {
    "blanket": {
      "pattern": "//^((?!(node_modules|test)).)*$/"
    },
    "travis-cov": {
      "threshold": 98
    }
  },
  "dependencies": {
    "URIjs": "^1.16.0",
    "async": "^1.4.2",
    "better-assert": "^1.0.2",
    "file-exists": "^0.1.1",
    "gerrit-event-emitter": "^0.1.5",
    "mktemp": "^0.4.0",
    "node-linguist": "^1.0.5",
    "procstreams": "^0.3.0",
    "xmldom": "^0.1.19",
    "xpath": "0.0.9"
  },
  "devDependencies": {
    "should": "*",
    "blanket": "*",
    "travis-cov": "*",
    "mocha-lcov-reporter": "*",
    "mocha": "*"
  },
  "keywords": [
    "gerrit",
    "checkstyle",
    "java"
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "author": "Roman Saveljev <roman.saveljev@haltian.com>",
  "license": "MIT"
}
